}
.p-cate-slogan{
  font-size: 26rpx;
  color: rgba(255,255,255,.5);
}
.p-cate-tl{
  font-size: 30rpx;
}
.p-cate-item image{
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  left: 20rpx;
  top: 50%;
  margin-top: -32rpx;
}
