.icons{
	background: #fff;
	width: 100%;	
	overflow: hidden;
  padding-top: 20rpx;
  margin-bottom: 14rpx;
}

.icons .icon {
	float: left;
	display: -webkit-flex;
	flex-direction: column;
	flex-wrap: wrap;
	width: 25%;
	text-align: center;
  padding-bottom: 30rpx;
  align-items: center;
}

.icons .icon image{
  width: 80rpx;
  height: 80rpx;
}
.icons .icon text {
	color: #666;
  display: inline-block;
  border-radius: 100%;
  font-size: 26rpx;
  margin-top: 10rpx;
}
.p-cate{
