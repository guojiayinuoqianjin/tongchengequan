.tel-wrap{
  background: #fff;
  margin: 20rpx 0;
  padding: 0 30rpx;
}
.tel-box{
  height: 120rpx;
  line-height: 120rpx;
  border-bottom: 1px solid #eee;
}
.tel-box:last-child{
  border-bottom: none;
}
.left{
  flex: 0 0 130rpx;
  width: 130rpx;
}
.right{
  flex: 0 0 250rpx;
  width: 250rpx;
  text-align: right;
}
.middle{
  flex: 1;
  color: #888888;
}
.btn{
  background: #0097ff;
  border-radius: 6rpx;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 28rpx;
  display: inline-block;
  color: #fff;
  font-size: 24rpx;
  overflow: inherit;
}
.hidden{
  display: none;
}
.validbtn{
  text-align: center;
  margin-top: 50rpx;
}
button[size="mini"] {
  font-size: 30rpx;
  height:80rpx;
  line-height: 80rpx;
  width:90%;

}