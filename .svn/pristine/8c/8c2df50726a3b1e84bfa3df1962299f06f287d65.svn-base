page{
  font-family: "PingFang SC","Helvetica Neue", Helvetica, STHeiTi, sans-serif;
  font-size: 28rpx;
  color: #333;
  background: #f3f3f5;
}
.border-1px-b,.border-1px-t{
  position: relative;
}
.border-1px-b::after,.border-1px-t::after{
  position: absolute;
  display: block;
  content: "";
  left: 0;
  width: 100%;
}
.border-1px-b::after{
  bottom: 0;
  border-top: 1px solid rgba(7, 17, 27, .1);
}
.border-1px-t::after{
  top: 0;
  border-top: 1px solid rgba(7, 17, 27, .1);
}
@media (-webkit-min-device-pixel-ratio: 1.5),(min-device-pixel-ratio: 1.5){
  .border-1px-b::after,.border-1px-t:after{
      -webkit-transform: scaleY(0.7);
      transform: scaleY(0.7);
  }
}
@media (-webkit-min-device-pixel-ratio: 2),(min-device-pixel-ratio: 2){
  .border-1px-b::after,.border-1px-t:after{
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
  }
}
.tips{
  color: #8a8a8a;
  text-align: center;
  font-size: 30rpx;
  margin: 20rpx 80rpx;
  display: flex;
  align-items: center;
}
.tips .noinfo-text{
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.tips .line{
  flex: 1;
  height: 1px;
  background: #e5e5e5;
}

view{
  box-sizing: border-box;
}
text{
  margin: 0;
  padding: 0;
}
.container{
  height: 100%;
  display: flex;
  flex-direction: column;
} 

.flexbox{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.flexrow{
  display: flex;
  align-items: center;
}
.iconfont{
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
}
.md-iconfont{
  width: 45rpx;
  height: 40rpx;
  vertical-align: middle;
}
.icon-zan{
  width: 45rpx;
  height: 40rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 45rpx 40rpx;
  display: inline-block;
  vertical-align: top;
}
.icon-zan-red{
  background-image: url("data:image/png;base64,");
}
.topbar{
  display: flex;
  flex-direction: row;
  height: 70rpx;
  line-height: 70rpx;
  position: fixed;
  top: 0;
  width: 100%;
  background: #fff;
  z-index: 999;
}
.topbar-item{
  flex: 1;
  text-align: center;
}
.topbaractive{
  color: #58ad45;
}
.new-item{
  padding: 30rpx 24rpx;
  display: flex;
  background: #fff;
  margin-bottom: 14rpx;
}
.avatar{
  width: 84rpx;
  height: 84rpx;
  flex: 0 0 84rpx;
}
.new-item-info{
  display: flex;
  flex-direction: column;
  padding-left: 20rpx;
  font-size: 30rpx;
  width: 100%;
  position: relative;
}
.new-item-name{
  color: #5b6f97;
  font-size: 30rpx;
}
.nickname{
  max-width: 300rpx;
  height: 50rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 5rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.new-item-dis{
  margin: 8rpx 0 12rpx;
  color: #737373;
}
.new-item-pic{
  margin-top: 5rpx;
}
.morepic .new-item-pic{
  margin-right: 10rpx;
  width: 158rpx;
  height: 158rpx;
}
.new-item-time{
  font-size: 26rpx;
  color: #737373;
  margin-top: 20rpx;
  height: 48rpx;
  line-height: 48rpx;
}
.new-item-name .iconfont{
  vertical-align: bottom;
}
.level{
  font-size: 22rpx;
  color: #ffb03b;
}
.useractive{
  margin-top: 20rpx;
  position: relative;
}
.user-item{
  margin-left: 14rpx;  
  vertical-align: middle;
  display: inline-block;
}
.inputtext{
  position: fixed;
  bottom: 0;
  width: 100%;
  left: 0;
  right: 0;
  border: 1px solid #bfbfbf;
  background: #fafafa;
  border-width: 1px 0;
  z-index: 10;
  display: flex;
  padding: 15rpx 30rpx;
  align-items: center;
}
.inputtext input{
  border: 1px solid #f1f1f1;
  border-radius: 10rpx;
  padding-left: 10rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #fcfcfc;
  flex: 8;
}
.inputtext .send{
  flex: 1;
  margin-left: 20rpx;
  line-height: 60rpx;
}
.new-item-des{
  margin-bottom: 20rpx;
  line-height: 1.2;
  margin-top: 12rpx;
}
.new-item-des-pre{
  white-space: pre-wrap;
  word-break:break-all;
}
.comment{
  background: #f3f3f5;
  margin-top: 16rpx;
  position: relative;
}
.trigon{
  position: absolute;
  width: 0;
  height: 0;
  border: 10rpx solid #f3f3f5;
  border-color: transparent transparent #f3f3f5 transparent;
  left: 20rpx;
  top: -20rpx;
}
.comment-list{
  padding: 0 20rpx 10rpx;
  display: flex;
  flex-direction: column;
}
.username{
