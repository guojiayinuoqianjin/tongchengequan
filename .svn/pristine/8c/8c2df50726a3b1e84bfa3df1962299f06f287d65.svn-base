}
.zannumber{
  margin-left: 5rpx;

}
.lvicon{
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
}
.onepic .new-item-pic{
  width: 500rpx;
}
.forbid-icon{
  position: absolute;
  width: 200rpx;
  height: 200rpx;
  right: 0;
  top: 0;
}
.forbid-info{
  color: red;
  margin: 20rpx 0;
}
.top{
  display: inline-block;
}
.top text{
