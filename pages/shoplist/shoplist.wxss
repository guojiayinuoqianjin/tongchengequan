.shop-l-cate-s{
  width: 100%;
  position: fixed;
  overflow-x: scroll;
  z-index: 9;
}
.shop-l-cate{
  height: 130rpx;
  background-color: #fff;
  padding: 14rpx 0;
  font-size: 0;
}
.shop-l-cate-pic{
  width: 70rpx;
  height: 70rpx;
}
.shop-l-cate-item{
  display: inline-block;
  width: 130rpx;
  text-align: center;
  font-size: 24rpx;
}
.shop-item-t-item-name{
  font-size:23rpx;
  height: 40rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  line-height: 40rpx;
  margin-bottom: 5rpx;
}

.container{
  padding-top: 135rpx;
}

.shop-item{
  background-color: #fff;
  padding: 20rpx 20rpx 8rpx;
  margin:10rpx;
  border-radius: 2%;
}
.shop-item-name{
  display: flex;
  align-items: center;
  background:#FFF;
  height:135rpx;

}
.shop-item-name-p{
  font-size:12px;
  font-weight: bold;
}

.shop-item-name-pic{
  width: 133rpx;
  height: 133rpx;
  flex: 0 0 133rpx;
}
.shop-item-name-info{
  padding-left: 20rpx;
  position: relative;
  flex: 1;
}
.shop-item-name-t{
  font-size: 24rpx;
  color: #888;
  margin:9rpx 0rpx;
}
.shop-item-name-t text{
  padding-right: 18rpx;
}
.shop-item-r-wrap{
  position: absolute;
  right: 0;
  top: 0;
  text-align: right;
  display: flex;
  align-items: center;
}
.shop-item-btn{
  width: 80rpx;
  height: 42rpx;
  line-height: 40rpx;
  border: 1px solid #fb531f;
  color: #fb531f;
  font-size: 24rpx;
  text-align: center;
  border-radius: 42rpx;
}
.shop-item-m{
  font-size: 24rpx;
  color: #888;
  padding-right: 10rpx;
}
.shop-item-t-s{
  overflow-x: scroll;
  margin-top:20rpx;
}
.shop-item-t{
  padding-top: 14rpx;
  margin-left: -4rpx;
}

.shop-item-t-item-one{
  width: 100%;
  float: left;
  padding-left: 10rpx;
}

.shop-item-t-item-one image{
  width: 100%;
  height: 240rpx;
  overflow: hidden;
  border-radius:2%;

}
.shop-item-t-item-two{
  width: 50%;
  float: left;
  padding-left: 10rpx;
}
.shop-item-t-item-two image{
  width: 100%;
  height: 240rpx;
  overflow: hidden;
  border-radius:2%;
}
.shop-item-t-item{
  width: 240rpx;
  float: left;
  padding-left: 10rpx;
}
.shop-item-t-item image{
  width: 100%;
  height: 240rpx;
  overflow: hidden;
}
image{
 border-radius:5%;
 height:240rpx;
}
.xl-sum{
  font-size: 24rpx;
  padding-right: 20rpx;
  color: #aaa;
}
.shop-address{
  width: 28rpx;
  height: 28rpx;
  vertical-align: middle;
  margin-right: 6rpx;
  margin-left: -3rpx;
}
.shop-item-name-info .hideStar{
    background-image: url("data:image/png;base64,");
    width: 43rpx;
    height: 43rpx;
    background-size: 43rpx 43rpx;
    background-position: center center;
    background-repeat: no-repeat;
    float: left;
}

.starsM{
    width: 43rpx;
    height: 43rpx;
    background-image: url("data:image/png;base64,");
    background-size: 43rpx 43rpx;
    background-position: center center;
    background-repeat: no-repeat;
    float: left;
}
.index-star{
  position: absolute;
  right: 0;
  top: 4rpx;
}
.shop-item-name-info .hideStar,.starsM{
  width: 28rpx;
  height: 28rpx;
  background-size: 28rpx 28rpx;
}
